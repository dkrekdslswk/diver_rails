<h1>blog index</h1>
<p><%= notice %></p>
<table>
    <tr>
        <th>title</th>
        <th>content</th>
    </tr>
    <% @blogs.each do |blog| %>
        <tr>
            <td><%= blog.title %></td>
            <td><%= blog.content %></td>
            <td><%= link_to "show blog", blog_path(blog.id) %></td>
            <td><%= link_to "edit blog", edit_blog_path(blog.id) %></td>
            <td><%= link_to "blog delete", blog_path(blog.id), method: :delete, data: { confirm: 'delete on more check' } %></td>
        </tr>
    <% end %>
</table>
<br>
<%= link_to 'create new blog', new_blog_path %>